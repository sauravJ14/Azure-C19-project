{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-reports-ni-adf"
		},
		"linked_blob_covid_report_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linked_blob_covid_report'"
		},
		"linked_dl_covidreport_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linked_dl_covidreport'"
		},
		"ls_db_covid_cluster_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_db_covid_cluster'"
		},
		"ls_sql_coviddb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_coviddb'"
		},
		"covid_death_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"linked_dl_covidreport_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingdlsj.dfs.core.windows.net/"
		},
		"ls_db_covid_cluster_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0723-124524-wrttxamj"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-reports-ni-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linked_blob_covid_report_connectionString": {
						"value": "[parameters('linked_blob_covid_report_connectionString')]"
					},
					"linked_dl_covidreport_accountKey": {
						"value": "[parameters('linked_dl_covidreport_accountKey')]"
					},
					"ls_db_covid_cluster_accessToken": {
						"value": "[parameters('ls_db_covid_cluster_accessToken')]"
					},
					"ls_sql_coviddb_connectionString": {
						"value": "[parameters('ls_sql_coviddb_connectionString')]"
					},
					"covid_death_data_properties_typeProperties_url": {
						"value": "[parameters('covid_death_data_properties_typeProperties_url')]"
					},
					"linked_dl_covidreport_properties_typeProperties_url": {
						"value": "[parameters('linked_dl_covidreport_properties_typeProperties_url')]"
					},
					"ls_db_covid_cluster_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_db_covid_cluster_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covid-reports-ni-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covid-reports-ni-adf_ArmTemplate_0')]"
			]
		}
	]
}